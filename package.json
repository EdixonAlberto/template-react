  "private": true,
  "license": "UNLICENSED",
  "version": "0.0.1",
  "type": "module",
  "author": "Edixon Piña",
  "contributors": [
    {
      "name": "Edixon Piña",
      "email": "edixonalbertto@gmail.com"
    }
  ],
  "scripts": {
